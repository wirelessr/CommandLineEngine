%define IF "(eth0|wlan0)";

%command
show capwap ap % info = {
	%privilege 100;
	%visibility 100;
	%function show_ap_info;
}

%command
show capwap ap % vlan = {
	%privilege 100;
	%visibility 100;
	%function show_ap_vlan;
}

%command
show interface % IF = {
	%privilege 100;
	%visibility 100;
	%function show_interface;
}

%command
show interface % all = {
	%privilege 100;
	%visibility 100;
	%function show_interface;
}
